  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "i18n-extract": "i18next --silent --output 'src/locales/ja/translations.json' 'src/**/*.{js,jsx,ts,tsx}'",
