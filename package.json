    "format-test": "pretter . --test",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "@types/lodash": "^4.14.190",
    "chart.js": "^4.0.1",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "http-proxy": "^1.18.1",
    "i18next": "^22.0.6",
    "i18next-parser": "^7.0.3",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "object-path-immutable": "^4.1.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-input-mask": "^2.0.4",
    "react-loading-overlay": "^1.0.1",
    "react-spinners": "^0.13.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
